    "chai": "3.5.0",
    "commitizen": "^2.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "mocha": "2.4.5",
    "istanbul": "0.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
